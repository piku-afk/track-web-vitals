{
  "printWidth": 100,
  "semi": true,
  "singleQuote": true,
  "useTabs": false,
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "<BUILTIN_MODULES>",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@components/(.*)$",
    "",
    "^@hooks/(.*)$",
    "",
    "^@utils/(.*)$",
    "",
    "^@constants/(.*)$",
    "",
    "^@/(.*)$",
    "",
    "^[./]"
  ],
  "importOrderTypeScriptVersion": "5.4.2"
}
